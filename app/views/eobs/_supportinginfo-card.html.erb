<div class='card bg-dark text-center'>
  <h2 class='card-header'>Supporting Information</h2>
  <div class='card-body'>
    <table class='table table-dark table-striped status caption-top'>
      <tbody>
        <% @eob.supportingInfo.each do |sequence, supportingInfo| %>
        
          <% if supportingInfo[:category] == ('In Network' || 'Out of Network' || 'Other Network') %>
            <caption><h3><%= supportingInfo[:category] %></h3></caption>
          <% else %>
            <tr>
              <th scope='row'><%= supportingInfo[:category] %>:</th>
              <td><%= supportingInfo[:info] %></td>
            </tr>
          <% end %>
        
        <% end %>
      </tbody>
    </table>
  </div>
</div>